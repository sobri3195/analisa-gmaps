<?xml version="1.0" encoding="utf-8"?>
<set
  xmlns:android="http://schemas.android.com/apk/res/android">
    <set android:interpolator="@android:anim/accelerate_interpolator" android:shareInterpolator="true">
        <objectAnimator android:duration="600" android:valueFrom="1.0" android:valueTo="4.64" android:valueType="floatType" android:propertyName="scaleX" />
        <objectAnimator android:duration="600" android:valueFrom="1.0" android:valueTo="4.64" android:valueType="floatType" android:propertyName="scaleY" />
    </set>
    <set android:interpolator="@android:anim/accelerate_interpolator" android:shareInterpolator="true" android:ordering="sequentially">
        <objectAnimator android:duration="300" android:startOffset="300" android:valueFrom="1.0" android:valueTo="0.0" android:valueType="floatType" android:propertyName="alpha" />
        <objectAnimator android:duration="1400" android:valueFrom="0.0" android:valueTo="0.0" android:valueType="floatType" android:propertyName="alpha" />
    </set>
</set>
