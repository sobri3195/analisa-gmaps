<?xml version="1.0" encoding="utf-8"?>
<set android:interpolator="@android:anim/linear_interpolator" android:shareInterpolator="true"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <set android:ordering="sequentially">
        <objectAnimator android:duration="800" android:valueFrom="1.0" android:valueTo="1.2" android:valueType="floatType" android:propertyName="scaleX" />
        <objectAnimator android:duration="800" android:valueFrom="1.2" android:valueTo="1.0" android:valueType="floatType" android:propertyName="scaleX" />
        <objectAnimator android:duration="1000" android:valueFrom="1.0" android:valueTo="1.0" android:valueType="floatType" android:propertyName="scaleX" />
    </set>
    <set android:ordering="sequentially">
        <objectAnimator android:duration="800" android:valueFrom="1.0" android:valueTo="1.2" android:valueType="floatType" android:propertyName="scaleY" />
        <objectAnimator android:duration="800" android:valueFrom="1.2" android:valueTo="1.0" android:valueType="floatType" android:propertyName="scaleY" />
        <objectAnimator android:duration="1000" android:valueFrom="1.0" android:valueTo="1.0" android:valueType="floatType" android:propertyName="scaleY" />
    </set>
</set>
